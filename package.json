{
  "name": "p7-react-host",
  "version": "1.0.0",
  "description": "The react variant of P7-riotjs-host",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "json": "cpx ./app/*.json                         dist       -v",
    "html": "cpx ./app/*.html                         dist       -v",
    "postBuild": "npm-run-all html json",
    "serve": "webpack-dev-server --env=dev --hot",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:prod": "webpack --env=prod --progress --profile --colors",
    "dev:0": "npm-run-all build:dev",
    "prod:0": "npm-run-all build:prod",
    "dev": "npm-run-all dev:0 postBuild serve",
    "prod": "npm-run-all dev:0 prod:0 postBuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghstahl/P7-react-host.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ghstahl/P7-react-host/issues"
  },
  "homepage": "https://github.com/ghstahl/P7-react-host#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babili": "^0.1.4",
    "babili-webpack-plugin": "^0.1.2",
    "compression-webpack-plugin": "^0.4.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.2",
    "es6-mixins": "^1.0.2",
    "eslint": "^3.19.0",
    "eslint-config-rallycoding": "^3.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.1",
    "redux-batched-actions": "^0.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.1",
    "redux-thunk": "^2.2.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "whatwg-fetch": "^2.0.3"
  }
}
